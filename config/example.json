{
  "url": "https://admin.example.com",
  "token": "your-access-token-here",
  "timeout": 8000,
  "headless": true,
  "retry": 2,
  "verbose": false,
  "routes": [
    { 
      "menuText": "首页", 
      "url": "https://admin.example.com/home" 
    },
    { 
      "menuText": "巡检中心", 
      "url": "https://admin.example.com/inspect",
      "screenshotScenarios": [
        {
          "type": "default",
          "description": "默认页面"
        },
        {
          "type": "tab",
          "text": "待处理",
          "description": "待处理 tab"
        },
        {
          "type": "tab",
          "text": "已完成",
          "description": "已完成 tab"
        },
        {
          "type": "modal",
          "trigger": "点击'新增巡检'按钮",
          "description": "新增巡检弹窗",
          "closeAfter": true
        }
      ]
    },
    { "menuText": "系统设置", "url": "https://admin.example.com/settings" }
  ],
  "autoDiscoverTabs": true,
  "tokenInjection": {
    "method": "cookie",
    "name": "access_token"
  },
  "localStorageItems": {
    "example_key": "example_value"
  },
  "screenshotComparison": {
    "enabled": false,
    "threshold": 0.1,
    "baselineDir": "./screenshots/baseline",
    "diffDir": "./screenshots/diff",
    "updateBaseline": false,
    "failOnDiff": false
  },
  "env": {
    "OPENAI_API_KEY": "sk-your-openai-api-key-here",
    "OPENAI_BASE_URL": "https://api.openai.com/v1",
    "MIDSCENE_MODEL_NAME": ""
  }
} 